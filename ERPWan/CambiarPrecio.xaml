<Window x:Name="windowCambiarPrecio" x:Class="ERPWan.CambiarPrecio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ERPWan"
        mc:Ignorable="d"
        Title="Cambiar Precio" FontSize="20" ResizeMode="NoResize" SizeToContent="WidthAndHeight" WindowStartupLocation="CenterScreen">
    <Grid Background="#FFC8C8C8" Height="150" Width="300">
        <Button x:Name="btnOK" Content="OK" Margin="0,0,10,10" Padding="5,5,5,5" TabIndex="4" HorizontalAlignment="Right" Width="90" Height="39" VerticalAlignment="Bottom" Click="btnOK_Click" IsDefault="True"/>
        <TextBox x:Name="txbPrecio" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" Width="230" TextChanged="txbPrecio_TextChanged"/>
        <Label Content="Precio" HorizontalAlignment="Center" Margin="0,24,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnCancelar" Content="Cancelar" Margin="0,0,200,10" Padding="5,5,5,5" TabIndex="4" HorizontalAlignment="Right" Width="90" Height="39" VerticalAlignment="Bottom" Click="btnCancelar_Click"/>
    </Grid>
</Window>

